extends layout

block content
  h1 #{title}
  nav
   a(href='getAllContact') Contacts
   a(href='getAll') AllContact
  form(action='saveData' method='get')
   .row
   label Name
   .row
    .input-field.col.s6
     input#first_name.validate(type='text', name='name')
   label Phone No.
   .row
    .input-field.col.s6
     input#first_name.validate(type='text', name='phone') 
   .row
    button.btn(type='submit') Save
  
